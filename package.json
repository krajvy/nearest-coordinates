{
  "name": "nearest-coordinates",
  "version": "2.3.1",
  "private": true,
  "description": "Find nearest given GPS coordinates on Earth from start point.",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krajvy/nearest-coordinates.git"
  },
  "keywords": [
    "nearest",
    "GPS",
    "coordinates"
  ],
  "author": "Jan Krivohlavek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krajvy/nearest-coordinates/issues"
  },
  "scripts": {
    "start": "export REACT_APP_VERSION=DEV-$(git rev-parse --short HEAD) && react-scripts start",
    "build": "export REACT_APP_VERSION=$(git rev-parse --short HEAD) && react-scripts build",
    "test": "react-scripts test",
    "test-all": "react-scripts test --watchAll=false",
    "format-check": "npx prettier --check .",
    "format-fix": "prettier -w ."
  },
  "engines": {
    "node": "^14.16.0",
    "npm": "^6.14.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.1.0",
    "react-loadingmask": "^4.0.6",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "2.2.1",
    "react-redux": "^7.2.4"
  }
}
