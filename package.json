{
  "name": "nearest-coordinates",
  "version": "2.3.1",
  "private": true,
  "description": "Find nearest given GPS coordinates on Earth from start point.",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krajvy/nearest-coordinates.git"
  },
  "keywords": [
    "nearest",
    "GPS",
    "coordinates"
  ],
  "author": "Jan Krivohlavek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krajvy/nearest-coordinates/issues"
  },
  "scripts": {
    "start": "export REACT_APP_VERSION=DEV-$(git rev-parse --short HEAD) && react-scripts start",
    "build": "export REACT_APP_VERSION=$(git rev-parse --short HEAD) && react-scripts build",
    "test": "react-scripts test",
    "test-all": "react-scripts test --watchAll=false",
    "format-check": "npx prettier --check .",
    "format-fix": "prettier -w ."
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "leaflet": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.0",
    "react-loadingmask": "^4.0.6",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.2"
  }
}
